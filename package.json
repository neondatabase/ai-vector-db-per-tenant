{
	"name": "ai-vector-db-per-tenant",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"cf-typegen": "wrangler types",
		"deploy": "bun run build && wrangler pages deploy",
		"dev": "remix vite:dev",
		"db:generate": "drizzle-kit generate --config=./app/lib/db/config.ts",
		"db:migrate": "drizzle-kit migrate --config=./app/lib/db/config.ts",
		"db:studio": "drizzle-kit studio --config=./app/lib/db/config.ts",
		"format": "bunx biome format --write .",
		"lint": "bunx biome lint --write .",
		"preview": "bun run build && wrangler pages dev --port=3000",
		"start": "wrangler pages dev ./build/client",
		"typecheck": "tsc",
		"typegen": "wrangler types",
		"vector-db-api:generate": "bunx openapi-typescript https://neon.tech/api_spec/release/v2.json -o ./app/lib/vector-db/api-schema.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.629.0",
		"@aws-sdk/s3-request-presigner": "^3.629.0",
		"@langchain/community": "^0.2.27",
		"@langchain/core": "^0.2.23",
		"@langchain/openai": "^0.2.6",
		"@neondatabase/serverless": "^0.9.4",
		"@radix-ui/react-avatar": "^1.1.0",
		"@remix-run/cloudflare": "^2.10.3",
		"@remix-run/cloudflare-pages": "^2.10.3",
		"@remix-run/react": "^2.10.3",
		"@tanstack/react-query": "^5.51.23",
		"ai": "^3.3.7",
		"clsx": "^2.1.1",
		"date-fns": "^3.6.0",
		"drizzle-orm": "^0.33.0",
		"drizzle-zod": "^0.5.1",
		"isbot": "^4.1.0",
		"nanoid": "^5.0.7",
		"openapi-fetch": "^0.11.1",
		"react": "^18.2.0",
		"react-aria-components": "^1.3.1",
		"react-dom": "^18.2.0",
		"remix-auth": "^3.7.0",
		"remix-auth-google": "^2.0.0",
		"remix-utils": "^7.6.0",
		"tailwind-merge": "^2.5.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@cloudflare/workers-types": "^4.20240806.0",
		"@remix-run/dev": "^2.10.3",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"autoprefixer": "^10.4.19",
		"drizzle-kit": "^0.24.0",
		"postcss": "^8.4.38",
		"remix-flat-routes": "^0.6.5",
		"tailwindcss": "^3.4.4",
		"tailwindcss-animate": "^1.0.7",
		"tailwindcss-react-aria-components": "^1.1.4",
		"typescript": "^5.1.6",
		"vite": "^5.1.0",
		"vite-tsconfig-paths": "^4.2.1",
		"wrangler": "^3.71.0"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
